{
  "expo": {
    "name": "انبار یار",
    "slug": "anbaryad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo.png",
    "scheme": "anbaryad",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0072FF",
        "foregroundImage": "./assets/img/logo.png",
        "backgroundImage": "./assets/img/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anbaryad.inventory"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/img/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/img/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0072FF",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05753242-7a6b-4957-8ebe-628e7d823f89"
      }
    }
  }
}
